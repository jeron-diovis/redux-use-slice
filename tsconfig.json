{
  "compilerOptions": {
    "jsx": "react",
    "allowJs": false,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["dom", "dom.iterable", "esnext"],
    "noEmit": true,
    "downlevelIteration": true,
    "sourceMap": false,
    "declaration": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "removeComments": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "types": [
      "vitest/globals"
    ]
  },
  "include": ["src"]
}
